{
  "validators": [
    {
      "field": "state",
      "validator": [
        {
          "required": true
        }
      ]
    },
    {
      "field": "country",
      "validator": [
        {
          "required": true
        }
      ]
    },
    {
      "field": "email",
      "validator": [
        {
          "validateEmail": true
        }
      ]
    },
    {
      "field": "name",
      "validator": [
        {
          "lengthRange": "4-10"
        },
        {
          "required": true
        }
      ]
    },
    {
      "field": "contact",
      "validator": [
        {
          "length": 10
        }
      ]
    }
  ]
}
